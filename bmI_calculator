/*
Create a BMI calculator using JavaScript functions. 
BMI = weight(KG)  / height2 (m2)    2  is squaredc

*/

function bmi (weight, height){


    //power Math.pow(number.number to times it by which in our case is two)
    const bmiCal = weight / Math.pow(height, 2) ;
   

return Math.round(bmiCal)

}

bmi(65, 1.8) // 20.0614
